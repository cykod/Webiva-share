<% active_table_for :share_links_table, @tbl do |t| -%>
<tr <%= highlight_row 'email',t.id %>>
  <td><%= entry_checkbox 'email',t.id %></td>
  <td><%= v(t.end_user ? t.end_user.full_name : 'Anonymous'.t)%></td>
  <td><%= v(t.end_user ? t.end_user.email : '-') %></td>
  <td><%= v(t.visitors) %></td>
  <td><%= t.created_at.strftime(DEFAULT_DATETIME_FORMAT.t) %></td>
</tr>
<% end -%>
